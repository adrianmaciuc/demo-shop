{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "users",
    "description": "Users",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": false
    },
    "lastName": {
      "type": "string",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "dateOfBirth": {
      "type": "date",
      "required": false
    },
    "gender": {
      "type": "enumeration",
      "enum": ["male", "female", "other", "prefer_not_to_say"],
      "required": false,
      "default": "prefer_not_to_say"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "addresses": {
      "type": "json",
      "required": false
    },
    "defaultShippingAddress": {
      "type": "json",
      "required": false
    },
    "defaultBillingAddress": {
      "type": "json",
      "required": false
    },
    "marketingConsent": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "preferences": {
      "type": "json",
      "required": false
    },
    "notes": {
      "type": "text",
      "required": false
    }
  }
}
